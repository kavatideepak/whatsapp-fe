{
  "expo": {
    "name": "synapse",
    "slug": "synapsedev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "synapse",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "infoPlist": {
        "NSContactsUsageDescription": "Synapse needs access to your contacts to help you connect with your teammates who are already using the app.",
        "NSContactsWriteUsageDescription": "Synapse needs permission to save your teammates to your phone's contacts for easy calling and messaging."
      }
    },
    "android": {
      "package": "com.synapse.app",
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
      "softwareKeyboardLayoutMode": "adjustResize",
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS"
      ],
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "86b20382-884a-4989-bb8e-b1f1e25b3845"
      }
    },
    "owner": "deepak_k"
  }
}
