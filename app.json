{
  "expo": {
    "name": "synapse",
    "slug": "synapse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "synapse",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSContactsUsageDescription": "Synapse needs access to your contacts to help you connect with your teammates who are already using the app.",
        "NSContactsWriteUsageDescription": "Synapse needs permission to save your teammates to your phone's contacts for easy calling and messaging."
      }
    },
    "android": {
      "package": "com.synapse.app",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": false,
      "predictiveBackGestureEnabled": false,
"android": {
  "package": "com.synapse.app",
  "softwareKeyboardLayoutMode": "adjustNothing",
  "config": {
    "windowSoftInputMode": "adjustResize|stateAlwaysHidden"
  }
},
      "permissions": [
        "READ_CONTACTS",
        "WRITE_CONTACTS"
      ],
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResourcesInReleaseBuilds": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f77e7e0a-5a0a-4008-84d2-61917da92daf"
      }
    }
  }
}
